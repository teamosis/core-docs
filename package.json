{
  "name": "@teamosis/core-docs",
  "description": "Core documentation setup for ThemeFisher, GetHugoThemes, and More",
  "version": "1.0.69",
  "author": {
    "name": "Mehedi Sharif Titas",
    "web": "https://gethugothemes.com"
  },
  "bugs": {
    "url": "https://github.com/teamosis/core-docs/issues"
  },
  "devDependencies": {
    "@davidsneighbour/browserslist-config": "^4.0.12",
    "@davidsneighbour/remark-config": "*",
    "@davidsneighbour/standard-version-config": "*",
    "@davidsneighbour/stylelint-config": "*"
  },
  "homepage": "https://github.com/teamosis/core-docs#readme",
  "keywords": [
    "hugo",
    "hugo-documentation",
    "hugo-site",
    "hugo-theme"
  ],
  "license": "UNLICENSED",
  "maintainers": [
    {
      "name": "Somrat Sorkar",
      "web": "https://somrat.netlify.app"
    },
    {
      "name": "Patrick Kollitsch",
      "email": "patrick@davids-neighbour.com",
      "web": "https://davids-neighbour.com"
    }
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamosis/core-docs.git"
  },
  "scripts": {
    "hugo": "hugo --i18n-warnings --templateMetrics --templateMetricsHints --verbose --verboseLog --path-warnings --enableGitInfo --log true --logFile hugo.log --verbose",
    "update": "hugo mod get -u ./... && hugo mod tidy",
    "release": "standard-version --release-as patch -a -t \"v\" && ./bin/release/postrelease",
    "release-major": "standard-version --release-as major -a -t \"v\" && ./bin/release/postrelease",
    "release-next": "standard-version --release-as minor -a -t \"v\" && ./bin/release/postrelease",
    "serve": "hugo server --disableFastRender --i18n-warnings --navigateToChanged  --templateMetrics --templateMetricsHints --verbose --verboseLog --path-warnings --buildDrafts --buildExpired --buildFuture --watch --enableGitInfo --forceSyncStatic --log true --logFile hugo.log --verbose"
  }
}
